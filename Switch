#include<iostream>
using namespace std;

int main() {
	int month;
	cout<<"Enter a number to know the corresponding month\n";
	cin>>month;
	
	switch(month) {
	case 1 :
    cout<<"january";
    break;
    
	case 2 :
	cout<<"february";
	break;
	
	case 3 :
	cout<<"march";
	break;			
		
	case 4 : 
	cout<<"april";
	break;
	
	case 5 :
	cout<<"may";
	break;
	
	case 6 :
	cout<<"april";
	break;
	
	default:
	cout<<"No more months in here go and check calender";
	}
}
